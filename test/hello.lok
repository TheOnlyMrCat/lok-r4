extern fn puts(*const c_char) -> c_int;
extern fn putchar(c_int) -> c_int;
extern fn printf(*const c_char...) -> c_int;

entry -> i32 {
	let at: i32 = 64;
	puts(c"Hello, world!");
	putchar(at); // @
	putchar(10); // \n
	let mut calc = 6 + 4 * 5;
	calc = calc - 4 / 2;
	printf(c"%s 6 + 4 * 5 - 4 / 2: %d\n", c"Result of", calc);
	return 0;
}